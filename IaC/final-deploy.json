{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "StorageAccountName": {
            "type": "String"
        },
        "servicePlanName": {
           "defaultValue": "maverick",
           "type": "string"
        },
        "functionAppName": {
           "defaultValue": "maverick",
           "type": "string"
        },
        "sourceRepo": {
           "defaultValue": "https://github.com/agnnn/azure-func.git",
           "type": "string"
        },
        "sourceBranch": {
           "defaultValue": "master",
           "type": "string"
        }
    },
    "variables": {
        "templateBaseUrl": "https://raw.githubusercontent.com/agnnn/azure-func/master",
        "templatelink": "[concat(variables('templateBaseUrl'), 'newStorageAccount.json')]"
    },
    "resources": [
        {
            "apiVersion": "2015-01-01",
            "name": "storageTemplate",
            "type": "Microsoft.Resources/deployments",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('templatelink')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "servicePlanName": {
                        "value": "[parameters('servicePlanName')]"
                    },
                    "functionAppName": {
                        "value": "[parameters('functionAppName')]"
                    },
                    "sourceRepo": {
                        "value": "[parameters('sourceRepo')]"
                    },
                    "sourceBranch": {
                        "value": "[parameters('sourceBranch')]"
                    },
                    "StorageAccountName": {
                        "value": "[parameters('StorageAccountName')]"
                    }
                }
            }
        }
    ]
}
